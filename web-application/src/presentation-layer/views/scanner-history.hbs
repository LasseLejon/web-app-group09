<h1>Scanner History</h1>
{{#if errors}}
    <p>Cannot get history because:</p>
    <ul>     
    {{#each errors}}
        <li>{{this}}</li>
    </ul>
    {{/each}}
{{else}}
    {{#if scannerBorrowSessionDetails}}
        <table border="solid">
            <tr>
                <th>Account Id</th>
                <th>Scanner Id</th>
                <th>Borrow Date</th>
                <th>Return Date</th>
            </tr>
            {{#each scannerBorrowSessionDetails}}
                <tr>
                    <td>{{accountId}}</td>
                    <td>{{scannerId}}</td>
                    <td>{{borrowDate}}</td>
                    <td>{{returnDate}}</td>
                </tr>
            {{/each}}
        </table>
    {{else}}
        <p>No history for scanners</p>
    {{/if}}
{{/if}}